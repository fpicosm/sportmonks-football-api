{
    "name": "fpicosm/sportmonks-football-api",
    "description": "A Laravel/Lumen wrapper for Sportmonks Football Api",
    "version": "1.0.1",
    "keywords": ["football", "sportmonks", "api", "sports"],
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Sportmonks\\FootballApi\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "authors": [
        {
            "name": "Fernando Picos",
            "email": "fpicosm@gmail.com",
            "homepage": "https://github.com/fpicosm"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "illuminate/support": "^9.34",
        "guzzlehttp/guzzle": "^7.5"
    },
    "require-dev": {
        "laravel/laravel": "^9.3",
        "phpunit/phpunit": "^9.5"
    }
}
