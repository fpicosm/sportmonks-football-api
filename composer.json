{
    "name": "fpicosm/sportmonks-football-api",
    "description": "A Laravel/Lumen wrapper for Sportmonks Football Api",
    "version": "2.1.0",
    "keywords": [
        "football",
        "sportmonks",
        "api",
        "sports"
    ],
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Sportmonks\\FootballApi\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "authors": [
        {
            "name": "Fernando Picos",
            "email": "fpicosm@gmail.com",
            "homepage": "https://github.com/fpicosm"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.8"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    },
    "require-dev": {
        "laravel/laravel": "^11.0",
        "phpunit/phpunit": "^11.1"
    }
}
